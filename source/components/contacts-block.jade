section.wrapper-big.contacts
    picture.hidden-mb
        source(type="image/webp" 
            media="(min-width: 1200px)"
            srcset="img/map-contact-desktop.webp 1x, img/map-contact-desktop@2x.webp 2x")
        source(type="image/webp" 
            srcset="img/map-contact-tablet.webp 1x, img/map-contact-tablet@2x.webp 2x")
        
        source(media="(min-width: 1200px)"
            srcset="img/map-contact-desktop.jpg 1x, img/map-contact-desktop@2x.jpg 2x")
        img(src="img/map-contact-tablet.jpg" 
            srcset="img/map-contact-tablet@2x.jpg 2x" width="760" height="700" 
            alt="Карта с адресом магазина: Санкт-Петербург, Торфяная дорога д.2, к.1, 3 этаж, секция 10")
    
    
    .wrapper
        .contacts__items
            .contacts__item
                .contacts__logo
                    include logo-gpsoff.jade
                p.contacts__city<strong>Санкт-Петербург</strong>
                p.contacts__adress м. Старая деревня<br />Торфяная дорога д.2, к.1, 3 этаж, секция 10
                p.contacts__phone<strong>8 (812) 244-86-93</strong>
                p.contacts__mail info@gpsoff.ru
